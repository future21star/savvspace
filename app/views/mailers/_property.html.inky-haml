/
  = item.inspect
%row
  %columns{large: '4'}
    = cl_image_tag(item.photos.first, type: :fetch, width: 170, class: 'small-float-center')
    %p.text-center
      %small
        = link_to('Property Details', '#')
  %columns{large: '8'}
    %h4= item.street_address
    %p= number_to_currency(item.list_price)
    %p
      = item.area
      \-
      = item.public_remarks
